{
  "arch": "riscv64",
  "cpu": "generic-rv64",
  "features": "+m,+a,+c",
  "llvm-target": "riscv64-unknown-linux-elf",
  "llvm-abiname": "lp64",
  "data-layout": "e-m:e-p:64:64-i64:64-i128:128-n32:64-S128",
  "target-endian": "little",
  "target-pointer-width": "64",
  "panic-strategy": "abort",
  "relocation-model": "static",
  "code-model": "medium",
  "linker": "rust-lld",
  "linker-flavor": "gnu-lld",
  "max-atomic-width": 64,
  "disable-redzone": true,
  "executables": true,
  "eh-frame-header": false,
  "emit-debug-gdb-scripts": false,
  "abi": "lp64",
  "vendor": "jolt",
  "os": "linux",
  "metadata": {
    "description": "RISC-V 64-bit for Jolt prover with Linux ABI (RV64IMAC ISA)",
    "tier": 3,
    "host-tools": false,
    "std": true
  },
  "env": "",
  "target-family": ["unix"],
  "has-rpath": false,
  "position-independent-executables": false,
  "dynamic-linking": false,
  "singlethread": true,
  "has-thread-local": false,
  "crt-static-default": true,
  "crt-static-respected": true,
  "supported-split-debuginfo": ["off"]
}
